{
"manifest_version": 2,
    "name": "Anti-Hate Speech Plugin",
    "description": "A plugin that censors offensive language and removes hateful tweets.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
